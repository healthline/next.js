{
  "name": "@kpdecker/next",
  "version": "12.0.1",
  "description": "Minimalistic framework for server-rendered React applications",
  "main": "./node/server/next.js",
  "license": "MIT",
  "repository": "zeit/next.js",
  "bugs": "https://github.com/zeit/next.js/issues",
  "homepage": "https://github.com/zeit/next.js",
  "files": [
    "bin",
    "node",
    "browser",
    "babel.js",
    "link.js",
    "build.js",
    "head.js",
    "document.js",
    "dynamic.js",
    "router.js",
    "error.js"
  ],
  "bin": {
    "next-build": "./bin/next-build"
  },
  "scripts": {
    "build": "yarn build:babel && yarn build:package",
    "build:babel": "yarn babel --out-dir node/babel  --source-maps=inline --plugins=@babel/plugin-transform-modules-commonjs babel",
    "build:package": "node ./node/babel/build-package.js",
    "release": "yarn build",
    "pretest": "npm run lint",
    "lint": "standard 'bin/*' 'client/**/*.js' 'examples/**/*.js' 'lib/**/*.js' 'server/**/*.js' 'test/**/*.js'",
    "prepublish": "npm run release",
    "precommit": "lint-staged"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/examples/**/lib/**"
    ]
  },
  "lint-staged": {
    "*.js": "standard",
    "bin/*": "standard"
  },
  "dependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-jsx-source": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.5",
    "@babel/template": "7.1.2",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-react-require": "3.0.1",
    "babel-plugin-transform-define": "1.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.20",
    "boom": "7.2.2",
    "chokidar": "2.0.4",
    "del": "3.0.0",
    "fs-readdir-recursive": "1.1.0",
    "glob": "7.1.3",
    "glob-promise": "3.4.0",
    "htmlescape": "1.1.1",
    "http-status": "1.3.1",
    "loader-utils": "1.1.0",
    "minimist": "1.2.0",
    "mv": "2.1.1",
    "mz": "2.7.0",
    "output-file-sync": "2.0.1",
    "path-match": "1.2.4",
    "prop-types": "15.6.2",
    "querystring": "0.2.0",
    "react-hot-loader": "4.3.12",
    "send": "0.16.2",
    "slash": "2.0.0",
    "strip-ansi": "5.0.0",
    "touch": "3.1.0",
    "url": "0.11.0",
    "uuid": "3.3.2",
    "webpack": "4.26.0",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "write-file-webpack-plugin": "4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "babel-eslint": "10.0.1",
    "lint-staged": "4.2.3",
    "node-notifier": "5.3.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "standard": "12.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ]
  }
}
