{
  "name": "@healthline/next",
  "version": "25.0.1",
  "description": "Minimalistic framework for server-rendered React applications",
  "main": "./node/server/next.js",
  "license": "MIT",
  "repository": "zeit/next.js",
  "bugs": "https://github.com/zeit/next.js/issues",
  "homepage": "https://github.com/zeit/next.js",
  "files": [
    "bin",
    "node",
    "browser",
    "babel.js",
    "link.js",
    "head.js",
    "document.js",
    "dynamic.js",
    "router.js",
    "error.js"
  ],
  "bin": {
    "next-build": "./bin/next-build"
  },
  "scripts": {
    "build": "yarn build:package",
    "build:package": "node -e 'require(`@healthline/six-million/build-package`)'",
    "release": "yarn build",
    "pretest": "npm run lint",
    "lint": "standard 'bin/*' 'client/**/*.js' 'examples/**/*.js' 'lib/**/*.js' 'server/**/*.js' 'test/**/*.js'",
    "prepublish": "npm run release",
    "precommit": "lint-staged"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/examples/**/lib/**"
    ]
  },
  "lint-staged": {
    "*.js": "standard",
    "bin/*": "standard"
  },
  "dependencies": {
    "del": "^5.1.0",
    "htmlescape": "^1.1.1",
    "minimist": "1.2.5",
    "mitt": "^1.2.0",
    "prop-types": "^15.7.0",
    "querystring": "^0.2.0",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@healthline/six-million": "6.0.1",
    "babel-eslint": "10.0.3",
    "lint-staged": "8.2.1",
    "node-notifier": "6.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "standard": "12.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ]
  }
}
